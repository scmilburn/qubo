<launch>
  <!--these actually launch the whole vehicle layer if we're on tortuga still need to add some things..-->
  <!-- <node name="camera_node" pkg="qubo_vehicle_layer" type="camera_node" args="tortuga" /> -->
  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find qubo_vehicle_layer)/rosconsole.conf"/>
  <node name="depth_node" pkg="qubo_vehicle_layer" type="depth_node" args="tortuga"/>
  <node name="dvl_node" pkg="qubo_vehicle_layer" type="dvl_node" args="tortuga" />
  <node name="imu_node" pkg="qubo_vehicle_layer" type="imu_node" args="tortuga" />
  <node name="led_node" pkg="qubo_vehicle_layer" type="led_node" args="tortuga" />
  <node name="power_node" pkg="qubo_vehicle_layer" type="power_node" args="tortuga"/>
  <node name="temp_node" pkg="qubo_vehicle_layer" type="temp_node" args="tortuga"/>
  <node name="movement_node" pkg="qubo_vehicle_layer" type="thruster_node" args="tortuga"/>
  <node name="thruster_node" pkg="qubo_vehicle_layer" type="thruster_node" args="tortuga" output="log"/>

  <node name="joy_node" type="qubo_teleop_joy" name="teleop">
  <node respawn="true" pkg="joy"
        type="joy" name="joy_node" >
    <param name="dev" type="string" value="/dev/input/js0" />
    <param name="deadzone" value="0.12" />
  </node>

</launch>
<!--change "log" to "screen" to move debug to stdout.-->
